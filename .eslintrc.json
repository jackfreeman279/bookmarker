{
    "env": {
        "es2020": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "System": "readonly",
        "test": true,
        "expect": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsdoc/recommended"
    ],
    "parserOptions":{
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "plugins": [ "jsdoc" ],
    "rules": {
        "react/jsx-curly-spacing": [
            2,
            { "when": "always", "children": true }
        ],
        "array-bracket-spacing": [
            "error",
            "always"
        ],
        "array-callback-return": [
            "error"
        ],
        "block-spacing": [
            "error"
        ],
        "brace-style": [
            "error",
            "1tbs",
            { "allowSingleLine": true }
        ],
        "camelcase": [
            "error"
        ],
        "complexity": [
            "warn"
        ],
        "comma-spacing": [
            "error",
            { "before": false, "after": true }
        ],
        "computed-property-spacing": [
            "warn",
            "always"
        ],
        "consistent-return": [
            "warn"
        ],
        "consistent-this": [
            "warn",
            "_self"
        ],
        "curly": [
            "error"
        ],
        //Does not fail if user has set // no default as the comment after last case
        "default-case": [
            "error"
        ],
        "dot-notation": [
            "warn",
            {"allowKeywords": true}
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "guard-for-in": [
            "error"
        ],
        "id-length": [
            "error",
            { "min": 2, "max" : 26, "exceptions": ["i", "$"] }
        ],
        "indent": [
            "error",
            4,
            { "SwitchCase" : 1 }
        ],
        "key-spacing": [
            "error"
        ],
        "keyword-spacing": [
            "error",
            {"before": true, "after": true}
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "max-lines": [
            "warn",
            { "max": 500, "skipComments": true }
        ],
        "new-cap": [
            "error",
            { "capIsNewExceptions": [
                    "getUrlGenerator",
                    "Log",
                    "Given",
                    "When",
                    "Then",
                    "BeforeAll",
                    "Before",
                    "After",
                    "AfterAll"
                ] }
        ],
        "new-parens": [
            "error"
        ],
        "no-alert": [
            "error"
        ],
        "no-caller": [
            "error"
        ],
        "no-console": [
            "warn",
            { "allow": [ "warn", "error", "info" ] }
        ],
        "no-else-return": [
            "warn"
        ],
        "no-empty-function": [
            "error"
        ],
        "no-eq-null": [
            "error"
        ],
        "no-eval": [
            "error"
        ],
        "no-extend-native": [
            "error"
        ],
        "no-extra-bind": [
            "error"
        ],
        "no-extra-label": [
            "error"
        ],
        "no-floating-decimal": [
            "error"
        ],
        "no-implied-eval": [
            "error"
        ],
        "no-iterator": [
            "error"
        ],
        "no-labels": [
            "error"
        ],
        "no-lone-blocks": [
            "error"
        ],
        "no-lonely-if": [
            "error"
        ],
        "no-loop-func": [
            "error"
        ],
        "no-magic-numbers": [
            "error",
            { "ignore": [-1, 0, 1, 2] }
        ],
        "no-mixed-operators": [
            "error"
        ],
        "no-multi-str": [
            "error"
        ],
        "no-multiple-empty-lines": [
            "error",
            { "max": 2, "maxBOF": 1, "maxEOF": 1 }
        ],
        "no-multi-spaces": [
            "warn"
        ],
        "no-new-func": [
            "error"
        ],
        "no-new-object": [
            "error"
        ],
        "no-new-wrappers": [
            "warn"
        ],
        "no-param-reassign": [
            "error"
        ],
        "no-proto": [
            "error"
        ],
        "no-return-assign": [
            "error",
            "except-parens"
        ],
        "no-script-url": [
            "error"
        ],
        "no-self-compare": [
            "error"
        ],
        "no-shadow": [
            "error"
        ],
        "no-shadow-restricted-names": [
            "error"
        ],
        "no-throw-literal": [
            "error"
        ],
        "no-trailing-spaces": [
            "error",
            { "skipBlankLines": true }
        ],
        "no-undef-init": [
            "error"
        ],
        "no-undefined": [
            "error"
        ],
        "no-unsafe-finally": [
            "error"
        ],
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "Given|When|Then|BeforeAll|Afterall",
                "argsIgnorePattern": "int|string"
            }
        ],
        "no-useless-call": [
            "warn"
        ],
        "no-useless-concat": [
            "error"
        ],
        "no-useless-escape": [
            "error"
        ],
        "no-void": [
            "error"
        ],
        "no-with": [
            "error"
        ],
        "operator-linebreak": [
            "error",
            "after"
        ],
        "quote-props": [
            "warn",
            "consistent-as-needed",
            { "keywords": true }
        ],
        "quotes": [
            "error",
            "single",
            { "avoidEscape": true }
        ],
        "semi": [
            "error",
            "always"
        ],
        "semi-spacing": [
            "error"
        ],
        "space-before-blocks": [
            "error"
        ],
        "space-in-parens": [
            "error", "always"
        ],
        "space-infix-ops": [
            "error"
        ],
        "valid-jsdoc": [
            "error",
            { "requireReturn": false }
        ],
        "wrap-iife": [
            "warn"
        ]
    }
}
